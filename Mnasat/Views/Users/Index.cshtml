@model IEnumerable<Mnasat.Models.Usr>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("New User", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Username)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Privilege)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Username)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Privilege)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.UsrID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.UsrID })
        </td>
    </tr>
}

</table>
