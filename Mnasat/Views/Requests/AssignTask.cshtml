@model IEnumerable<Mnasat.Models.Team>

@{
    ViewBag.Title = "Assign Task";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Assign Task To Team</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.TeamName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.TeamName)
        </td>
        <td>
            @Html.ActionLink("Assign", "AssignConfirmed", new { id=item.TeamID })
        </td>
    </tr>
}

</table>
